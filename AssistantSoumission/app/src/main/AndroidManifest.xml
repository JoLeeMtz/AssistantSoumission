<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.soumission.assistant.assistantsoumission">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_not_tranparent"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Page_ModifyItem" />
        <activity android:name=".Page_AddItem" />
        <activity android:name=".Page_ModifyClient" />
        <activity android:name=".Page_AddClient"></activity>
    </application>

</manifest>